--Display the database in it's entirety 
SELECT * FROM makeup; 

--What is the average price of items in makeup store?
SELECT AVG(price) FROM makeup; 

--How are higher priced items rated?
SELECT item, price, rating FROM makeup WHERE price >= 10 ORDER BY rating;
