--Create a makeup store database

CREATE TABLE "makeup" (id INTEGER PRIMARY KEY, item TEXT, quantity INTEGER, price INTEGER, rating INTEGER); 

INSERT INTO makeup VALUES (1, "mascara", 15, 11, 4);
